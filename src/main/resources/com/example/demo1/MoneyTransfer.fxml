        <TextField fx:id="amount_bar" layoutX="234.0" layoutY="71.0" prefHeight="47.0" prefWidth="194.0" promptText="amount" />
        <TextField fx:id="account_no" layoutX="234.0" layoutY="143.0" prefHeight="47.0" prefWidth="194.0" promptText="account no" />
        <Label layoutX="120.0" layoutY="70.0" prefHeight="47.0" prefWidth="104.0" text="Amount :">
            <font>
                <Font size="16.0" />
            </font>
        </Label>
        <Label layoutX="120.0" layoutY="143.0" prefHeight="47.0" prefWidth="104.0" text="Account no :">
         <font>
            <Font size="16.0" />
         </font>
        </Label>
      <TextField fx:id="password_bar" layoutX="234.0" layoutY="213.0" prefHeight="47.0" prefWidth="194.0" promptText="password" />
      <Label layoutX="120.0" layoutY="213.0" prefHeight="47.0" prefWidth="104.0" text="Password :">
         <font>
            <Font size="16.0" />
         </font>
      </Label>
        <Button fx:id="backbutton" layoutX="33.0" layoutY="22.0" mnemonicParsing="false" onAction="#BackbuttonClicked" prefHeight="21.0" prefWidth="68.0" text="Back" />
    </children>
</AnchorPane>
