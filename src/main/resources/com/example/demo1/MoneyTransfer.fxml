<AnchorPane prefHeight="400.0" prefWidth="483.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demo1.MoneyTransferController">
    <children>
        <Accordion layoutX="228.0" layoutY="110.0" />
